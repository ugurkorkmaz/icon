{
  "name": "meticon",
  "description": "A Google meterial icon library for Svelte",
  "keywords": [
    "svelte",
    "sveltekit",
    "sveltekit-component",
    "icon",
    "icons",
    "meterial",
    "google-meterial-icons",
    "google-meterial-icons-svelte",
    "google-meterial-icons-sveltekit"
  ],
  "version": "0.0.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ugurkorkmaz/meticon.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.ts"
    },
    "./Icon.svelte": {
      "types": "./dist/Icon.svelte.d.ts",
      "svelte": "./dist/Icon.svelte"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.2.0"
  },
  "svelte": "./dist/index.ts",
  "types": "./dist/index.d.ts",
  "type": "module"
}
